{% if type != 'Author' %}

    public function set{{ pluralType|capitalize }}(Collection ${{ type|lower }}): self
    {
        $this->{{ pluralType }} = ${{ type|lower }};

        return $this;
    }

    public function get{{ pluralType|capitalize }}(): Collection
    {
        return $this->{{ pluralType }};
    }
{% else %}

     public function set{{ type|capitalize }}(?\{{ namespace }}\Entity\{{ entity_class }}{{ type }} ${{ type|lower }}): self
     {
         $this->{{ type|lower }} = ${{ type|lower }};

         return $this;
     }

      public function get{{ type|capitalize }}(): ?\{{ namespace }}\Entity\{{ entity_class }}{{ type }}
      {
          return $this->{{ type|lower }};
      }
{% endif %}
